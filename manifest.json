{
    "name": "TwitterRemoveImpression",
    "version": "1.0.0",
    "manifest_version": 3,
    "description": "Remove Impression from Twitter",
    "content_scripts": [{
      "run_at": "document_end",
      "matches": ["https://twitter.com/*"],
      "js": [
        "content.js"
      ]
    }]
  }
